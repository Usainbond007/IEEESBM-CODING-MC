class Solution {
    public int maxSubArray(int[] nums) 
    {
        //lets use kadanes algo
        
        int csum=nums[0];
        int msum=nums[0];
        for(int i=1;i<nums.length;i++)
        {
             csum=Math.max(nums[i],csum+nums[i]);
             msum=Math.max(csum,msum);
        }

return msum;
    }
}
 https://leetcode.com/problems/maximum-subarray/
